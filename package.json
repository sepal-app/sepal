    "@tailwindcss/line-clamp": "0.4.4",
    "@tailwindcss/typography": "0.5.9",
    "@uppy/core": "^3.1.2",
    "@uppy/dashboard": "^3.3.2",
    "@uppy/xhr-upload": "^3.1.1",
    "alpinejs": "3.12.0",
    "autoprefixer": "10.4.14",
    "cssnano": "6.0.0",
    "esbuild": "0.17.14",
    "htmx.org": "1.8.6",
    "npm-run-all": "^4.1.5",
