  "scripts": {
    "build:js:shared.js": "esbuild sepal/static/js/shared.js --bundle --minify --format=esm --sourcemap --outfile=sepal/static/js/shared.min.js",
    "build:js:upload.js": "esbuild sepal/media/templates/media/_upload.js --bundle --minify --format=esm --sourcemap --outfile=sepal/media/templates/media/_upload.min.js",
    "build:js": "run-s build:js:shared.js build:js:upload.js",
    "watch:js": "run-p \"build:js:* -- --watch=forever\"",
    "build:css": "postcss sepal/static/css/main.css -o sepal/static/css/main.min.css --verbose",
    "watch:css": "run-p \"build:css -- --watch\"",
    "build": "run-s build:*",
    "watch": "run-p watch:*"
  },
